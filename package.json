{
  "name": "starter-kit",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "start:dev": "cross-env DEV_HOST=172.16.3.14 npm start",
    "build": "cross-env NODE_ENV=production webpack --progress --colors",
    "test": "cross-env NODE_ENV=test mocha --require src/utils/test/browser.js \"src/**/*.spec.js\"",
    "test:file-reporter": "cross-env NODE_ENV=test XUNIT_FILE=build/test-report.xml mocha --require src/utils/test/browser.js --reporter xunit-file \"src/**/*.spec.js\"",
    "test:watch": "cross-env NODE_ENV=test nodemon --exec 'mocha --require src/utils/test/browser.js --reporter dot \"src/**/*.spec.js\"'",
    "test:coverage": "cross-env NODE_ENV=test nyc npm test",
    "clean": "rm -rf build .nyc_output coverage"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".jsx",
      ".js"
    ]
  },
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "axios": "^0.15.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
